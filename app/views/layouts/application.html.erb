<!DOCTYPE html>
<html>
  <head>
    <title>WarehouseApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%#= stylesheet_pack_tag 'application' %>
    <%= javascript_pack_tag 'application'
                            # 'data-turbolinks-track': true, async: false, defer: true
    %>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <%= stylesheet_link_tag "authorization" %>

  </head>

  <body >
    <p class="notice m-0 p-0"><%= notice %></p>
    <p class="alert m-0 p-0"><%= alert %></p>

    <%= react_component("MainHeader", props: {
          isSignedIn: user_signed_in?,
          currentUser: current_user
        }) %>
    <%= yield %>
  </body>
</html>
